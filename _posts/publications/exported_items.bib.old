	pmcid = {PMC4515363},
	keywords = {*Cell Differentiation/genetics, *Chromatin Assembly and Disassembly/genetics, Alleles, Allelic Imbalance/genetics, Cell Lineage/genetics, Chromatin/*chemistry/genetics/*metabolism, Embryonic Stem Cells/*cytology/*metabolism, Enhancer Elements, Genetic/genetics, Epigenesis, Genetic/*genetics, Epigenomics, Gene Regulatory Networks, Humans, Promoter Regions, Genetic/genetics, Reproducibility of Results}
