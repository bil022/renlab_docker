	pmcid = {PMC2867844},
	keywords = {*Epigenesis, Genetic, Cell Line, Cell Lineage/*genetics, Chromatin/genetics, {CpG} Islands/genetics, {DNA} Methylation/genetics, Embryonic Stem Cells/cytology/metabolism, Fibroblasts/*cytology/*metabolism, Genes, Developmental, Genome, Human/*genetics, Histones/metabolism, Humans, Lysine/metabolism, Pluripotent Stem Cells/*cytology/*metabolism, Protein Processing, Post-Translational, Regulatory Sequences, Nucleic Acid/genetics}
