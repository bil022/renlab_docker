	author = {Lo, Miao-Chia and Peterson, Luke F. and Yan, Ming and Cong, Xiuli and Jin, Fulai and Shia, Wei-Jong and Matsuura, Shinobu and Ahn, Eun-Young and Komeno, Yukiko and Ly, Minh and Ommen, Hans B. and Chen, I.-Ming and Hokland, Peter and Willman, Cheryl L. and Ren, Bing and Zhang, Dong-Er},
	date = {2012-08-16},
	pmid = {22740448},
	pmcid = {PMC3423785},
	keywords = {*Gene Expression Profiling, *Translocation, Genetic, Animals, Antigens, {CD}45/metabolism, Antineoplastic Agents/therapeutic use, Cell Line, Tumor, Cell Transformation, Neoplastic/genetics/pathology, Chromatin Immunoprecipitation, Chromosomes, Human, Pair 8/*genetics, Chromosomes, Human, Pair 21/*genetics, {DNA}, Neoplasm/*metabolism, Enzyme Activation, Gene Expression Regulation, Leukemic, Gene Regulatory Networks/genetics, Genes, Neoplasm/genetics, Humans, Janus Kinases/metabolism, Leukemia, Myeloid, Acute/*drug therapy/*genetics, Mice, Oligonucleotide Array Sequence Analysis, Promoter Regions, Genetic/genetics, Reproducibility of Results, Signal Transduction/genetics, {STAT} Transcription Factors/metabolism}
